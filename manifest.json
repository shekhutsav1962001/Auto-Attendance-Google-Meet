{
    "name": "auto attendance",
    "description": "auto attendance by utsav shekh",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": ["https://meet.google.com/*"],
            "js": [ "content.js"]
        }
    ],
    "host_permissions": [
        "https://meet.google.com/*"
    ]
}